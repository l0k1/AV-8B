<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

    <path>pylons.ac</path>
    <animation>
        <object-name>pylon1.L</object-name>
        <object-name>pylon2.L</object-name>
        <object-name>pylon3.L</object-name>
        <object-name>pylon4.L</object-name>
        <object-name>pylon5.L</object-name>
        <object-name>pylon.C</object-name>
        <object-name>pylon5.R</object-name>
        <object-name>pylon4.R</object-name>
        <object-name>pylon3.R</object-name>
        <object-name>pylon2.R</object-name>
        <object-name>pylon1.R</object-name>
    </animation>

    <animation>
        <type>select</type>
        <object-name>pylon3.L</object-name>
        <condition>
            <equals>
                <value>1</value>
                <value>0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>pylon5.L</object-name>
        <condition>
            <equals>
                <value>1</value>
                <value>0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>pylon.C</object-name>
        <condition>
            <equals>
                <value>1</value>
                <value>0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>pylon5.R</object-name>
        <condition>
            <equals>
                <value>1</value>
                <value>0</value>
            </equals>
        </condition>
    </animation>

    <animation>
        <type>select</type>
        <object-name>pylon3.R</object-name>
        <condition>
            <equals>
                <value>1</value>
                <value>0</value>
            </equals>
        </condition>
    </animation>

</PropertyList>
