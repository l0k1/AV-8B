  <force name="propulsion/pitch-front" frame="BODY">
    <function>
      <!-- if nozzle > 0.4/36 deg and elevator is negative, thrust = abs(elevator) * 1000, else thrust = 0 -->
      <ifthen>
        <and>
          <gt>
            <property>fcs/nozzle-pos-norm</property>
            <value>0.4</value>
          </gt>
          <lt>
            <property>fcs/elevator-cmd-norm</property>
            <value>0</value>
          </lt>
        </and>
        <product>
          <abs>
            <property>fcs/elevator-cmd-norm</property>
          </abs>
          <value>1000</value> <!-- this is the max thrust force -->
        </product>
        <value>0</value>
      </ifthen>
    </function>
