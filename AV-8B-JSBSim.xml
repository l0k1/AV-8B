 <aerodynamics>

  <function name="aero/Vj">
    <description>Jet Velocity Ratio</description>
    <pow>
      <quotient>
        <product>
          <value>13</value>
          <property>aero/qbar-psf</property>
        </product>
        <max>
          <property>propulsion/engine[0]/thrust-lbs</property>
          <value>1</value>
        </max>
      </quotient>
      <value>0.5</value>
    </pow>
  </function>

  <function name="aero/k">
    <description>Not sure but NASA thought it was important</description>
    <quotient>
      <difference>
        <property>aero/Vj</property>
        <value>0.05</value>
      </difference>
      <value>0.05</value>
    </quotient>
  </function>

  <function name="aero/alpha-rad-corr">
    <description>Alpha corrected with Vj</description>
    <documentation>
      if vj greater than or equal to 0.1 then alpha = alpha
      if vj is between 0.05 and 0.1 then alpha = pitch + (alpha - pitch) * k
      if vj less than or equal to 0.05 then alpha = pitch
    </documentation>
    <ifthen>
      <ge>
        <property>aero/Vj</property>
        <value>0.1</value>
      </ge>
      <property>aero/alpha-rad</property>
      <ifthen>
        <le>
          <property>aero/Vj</property>
          <value>0.05</value>
        </le>
        <property>attitude/pitch-rad</property>
        <sum>
          <property>attitude/pitch-rad</property>
          <product>
            <difference>
              <property>aero/alpha-rad</property>
              <property>attitude/pitch-rad</property>
            </difference>
            <property>aero/k</property>
          </product>
        </sum>
      </ifthen>
    </ifthen>
  </function>

  <function name="aero/alpha-20">
    <description>alpha-corr divided by 20</description>
    <quotient>
      <todegrees>
        <property>aero/alpha-rad-corr</property>
      </todegrees>
      <value>20</value>
    </quotient>
  </function>

  <function name="aero/beta-deg1">
    <description></description>
    <todegrees>
      <property>aero/beta-rad</property>
    </todegrees>
  </function>


  <axis name="LIFT">

    <!-- Lift above 0.85 and below -0.85 is generalised -->
    <!-- -5 to 25 degrees from https://ntrs.nasa.gov/archive/nasa/casi.ntrs.nasa.gov/19880003981.pdf -->
    <function name="aero/force/Lift_alpha">
      <description>Lift due to alpha</description>
      <product>
          <property>aero/qbar-psf</property>
          <property>metrics/Sw-sqft</property>
          <sum>
            <product>
              <value>-0.456</value>
              <pow>
                <property>aero/alpha-20</property>
                <value>3</value>
              </pow>
            </product>
            <product>
              <value>0.307</value>
              <pow>
                <property>aero/alpha-20</property>
                <value>2</value>
              </pow>
            </product>
            <product>
              <value>1.465</value>
              <property>aero/alpha-20</property>
            </product>
          </sum>
          <property>tune/lift/alpha</property>
      </product>
    </function>

    <function name="aero/force/Lift_pitch_rate">
